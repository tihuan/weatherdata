<table class="temp-table">
    <thread>
      <tr>
        <% weathers.map do |weather| %>
          <th><%= weather.localtime.strftime("%a %I:%M%p") %></th>
        <% end %>
      </tr>
    </thread>
    <tbody>
      <% weathers.each do |weather| %>
        <td><%= weather.temp %>F</td>
      <% end %>
    </tbody>
</table>
